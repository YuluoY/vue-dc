# EditorConfig is awesome: https://EditorConfig.org

# 顶级配置文件
root = true

# 通用配置
[*]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true

# Markdown 文件
[*.md]
trim_trailing_whitespace = false

# JSON 文件
[*.json]
indent_size = 2

# JavaScript 文件
[*.js]
indent_size = 2

# Vue 文件
[*.vue]
indent_size = 2

# YAML 文件
[*.yml]
indent_size = 2